<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>Zeem</TargetDatabaseName>
    <DeployScriptFileName>Zeem.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=majid.database.windows.net;Persist Security Info=True;User ID=majid;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=False;TrustServerCertificate=False</TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <DatabaseEdition>Basic</DatabaseEdition>
    <DatabaseServiceObjective>Basic</DatabaseServiceObjective>
    <DatabaseMaximumSize>2</DatabaseMaximumSize>
    <DropObjectsNotInSource>True</DropObjectsNotInSource>
    <DoNotDropLogins>True</DoNotDropLogins>
    <DoNotDropServerRoles>True</DoNotDropServerRoles>
    <DoNotDropServerTriggers>True</DoNotDropServerTriggers>
    <DoNotDropSequences>True</DoNotDropSequences>
    <DoNotDropRoleMembership>True</DoNotDropRoleMembership>
    <DoNotDropUsers>True</DoNotDropUsers>
    <DoNotDropUserDefinedTableTypes>True</DoNotDropUserDefinedTableTypes>
    <DoNotDropUserDefinedDataTypes>True</DoNotDropUserDefinedDataTypes>
  </PropertyGroup>
</Project>